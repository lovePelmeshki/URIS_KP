<Page x:Class="URIS_KP.View.EmployeePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:URIS_KP.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EmployeePage" ShowsNavigationUI="False">
    <Page.Resources>
        <Style x:Key="stdBtn" TargetType="Button">
            <Setter Property="Width" Value="160"/>
            <Setter Property="Height" Value="36"/>
        </Style>
    </Page.Resources>

    <Grid>
        <DockPanel LastChildFill="True">
            <Button x:Name="buttonAddNewEmployee" DockPanel.Dock="Bottom" Height="36" Width="150"/>
            <DataGrid x:Name="dataGridEmployeePage" DockPanel.Dock="Top">

            </DataGrid>
        </DockPanel>

        
    </Grid>
</Page>
